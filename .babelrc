{
    "comments": false,
    "sourceMaps": true,
    "retainLines": true,
    "presets": ["env"],
    "env": {
        "main": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "node": "12.4.0"
                        }
                    }
                ]
            ]
        },
        "renderer": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "electron": "8.3.0"
                        }
                    }
                ]
            ]
        }
    },
    "plugins": ["transform-runtime", "transform-object-rest-spread"]
}
