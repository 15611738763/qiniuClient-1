{
    "comments": false,
    "sourceMaps": true,
    "retainLines": true,
    "env": {
        "main": {
            "presets": [
                "env",
                {
                    "modules": false,
                    "targets": {
                        "node": 7
                    }
                }
            ]
        },
        "renderer": {
            "presets": [
                [
                    "env",
                    {
                        "modules": false,
                        "targets": {
                            "electron": "3.0.4"
                        }
                    }
                ],
                "stage-0"
            ]
        }
    },
    "plugins": [
        "transform-runtime"
    ]
}